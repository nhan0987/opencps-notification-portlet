<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.2.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_2_0.dtd">

<liferay-portlet-app>
	<portlet>
		<portlet-name>113</portlet-name>
		<poller-processor-class>org.opencps.notification.dockbarnotifications.poller.DockbarNotificationsPollerProcessor</poller-processor-class>
		<preferences-owned-by-group>true</preferences-owned-by-group>
		<use-default-template>false</use-default-template>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<footer-portlet-javascript>/js/main.js</footer-portlet-javascript>
		<css-class-wrapper>113-opencps-notification-portlet</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>114</portlet-name>
		<struts-path>manage_notifications</struts-path>
		<use-default-template>false</use-default-template>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<footer-portlet-javascript>/js/main.js</footer-portlet-javascript>
		<css-class-wrapper>114-manage-notification-portlet</css-class-wrapper>
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
		<role-link>Guest</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>power-user</role-name>
		<role-link>Power User</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>user</role-name>
		<role-link>User</role-link>
	</role-mapper>

	
</liferay-portlet-app>